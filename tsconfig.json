{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "lib": ["ES2022", "DOM"],
    "allowJs": true,
    "types": ["node"],
    "skipLibCheck": true,
    "allowImportingTsExtensions": false,
    "noEmit": false
  }
}